{
  "name": "cypress-mocking",
  "version": "2.0.1",
  "description": "This is for mocking api response for react application",
  "author": "Shyedhu",
  "repository": {
    "type": "git",
    "url": "https://github.com/shyedhu/cypress-mocking.git"
  },
  "scripts": {
    "cy:open": "./node_modules/.bin/cypress open",
    "cy:run": "./node_modules/.bin/cypress run -e ENV=https://shyedhu-react-app-3000.herokuapp.com/login",
    "cy:run:chrome": "./node_modules/.bin/cypress run -b chrome -e ENV=https://shyedhu-react-app-3000.herokuapp.com/login",
    "cy:run:firefox": "./node_modules/.bin/cypress run -b firefox -e ENV=https://shyedhu-react-app-3000.herokuapp.com/login",
    "cy:run:headless": "./node_modules/.bin/cypress run --headless -b chrome -e ENV=https://shyedhu-react-app-3000.herokuapp.com/login",
    "cy:open:win": "node_modules\\.bin\\cypress open",
    "cy:run:win": "node_modules\\.bin\\cypress run -e ENV=https://shyedhu-react-app-3000.herokuapp.com/login",
    "cy:run:chrome:win": "node_modules\\.bin\\cypress run -b chrome -e ENV=https://shyedhu-react-app-3000.herokuapp.com/login",
    "cy:run:firefox:win": "node_modules\\.bin\\cypress run -b firefox -e ENV=https://shyedhu-react-app-3000.herokuapp.com/login",
    "lint:run": "eslint '**/*.js'",
    "lint:fix": "eslint --fix '**/*.js'"
  },
  "dependencies": {
    "cypress": "7.4.0",
    "ls": "^0.2.1",
    "mocha": "^5.2.0",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^4.0.0",
    "rimraf": "^3.0.2",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "cypress-log-to-output": "^1.0.8",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4"
  }
}
